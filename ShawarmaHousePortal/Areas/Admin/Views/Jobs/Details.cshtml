@model ShawarmaHousePortal.Models.Job

@{
    ViewData["Title"] = "Details";
}
<nav aria-label="breadcrumb">
    <ol class="breadcrumb bg-transparent mb-0 pb-0 pt-1 px-0 ">
        <li class="breadcrumb-item text-sm ps-2"><a class="opacity-5 text-dark" asp-area="Admin" asp-controller="Home" asp-action="Index">الرئيسية </a></li>
        <li class="breadcrumb-item text-sm ps-2"><a class="opacity-5 text-dark" asp-area="Admin" asp-controller="Home" asp-action="Index">الوظائف </a></li>
        <li class="breadcrumb-item text-sm text-dark active" aria-current="page">تفاصيل</li>
    </ol>
</nav>
<br />


<div class="card">
    <h4 class="card-header">تفاصيل الوظيفة</h4>
    <hr />
    <dl class="row card-body">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.JobName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.JobName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.JonPranch)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.JonPranch)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.JobCreateDate)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.JobCreateDate)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.JobSatuse)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.JobSatuse)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.JobDescription)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.JobDescription)
        </dd>
    </dl>
    <div class="card-footer">
        <a class="btn btn-sm btn-info" asp-action="Edit" asp-route-id="@Model?.JobId">Edit</a> |
        <a class="btn btn-sm btn-primary" asp-action="Index">Back to List</a>
    </div>
</div>

